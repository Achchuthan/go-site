target/gafs/wb/wb-src.gaf.gz: 
	wget --no-check-certificate http://geneontology.org/gene-associations/submission/gene_association.wb.gz -O $@.tmp && mv $@.tmp $@ && touch $@

target/gafs/wb/wb-filtered.gaf: target/gafs/wb/wb-src.gaf.gz
	gzip -dc $< | ./util/new-filter-gaf.pl -m target/datasets-metadata.json -p wb -e $@.errors -r $@.report > $@.tmp && mv $@.tmp $@

target/gafs/wb/wb-gafcheck: target/gafs/wb/wb-src.gaf.gz
	$(OWLTOOLS_GAFCHECK)

target/gafs/wb/wb-filtered.gpad: target/gafs/wb/wb-filtered.gaf
	owltools --gaf $< --write-gpad -o $@.tmp && mv $@.tmp $@

target/gafs/wb/wb.gpi: target/gafs/wb/wb-filtered.gaf
	owltools --gaf $< --write-gpi -o $@.tmp && mv $@.tmp $@

target/gafs/wb/wb.ttl: target/gafs/wb/wb-filtered.gaf $(ONT_MERGED)
	MINERVA_CLI_MEMORY=16G minerva-cli.sh $(ONT_MERGED) --gaf $< --gaf-lego-individuals --skip-merge --format turtle -o $@.tmp && mv $@.tmp $@

